/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@infra/*": [
        "src/app/_infra/*"
      ],
      "@models/*": [
        "src/app/_infra/core/models/*"
      ],
      "@core/*": [
        "src/app/_infra/core/*"
      ],
      "@ui/*": [
        "src/app/_infra/ui/*"
      ],
      "@board/*": [
        "src/app/board/*"
      ],
      "@boardinfra/*": [
        "src/app/board/_infra/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
